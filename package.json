{
  "name": "sznm.dev",
  "private": true,
  "description": "Agustinus Nathaniel's dev site",
  "version": "1.9.0",
  "author": "sozonome",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true yarn build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.0.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@giscus/react": "^2.0.4",
    "@notionhq/client": "^1.0.4",
    "chakra-ui-markdown-renderer": "^4.0.0",
    "cors": "^2.8.5",
    "framer-motion": "^6.3.3",
    "gray-matter": "^4.0.3",
    "next": "12.1.5",
    "next-seo": "^5.4.0",
    "preact": "^10.7.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-icons": "^4.3.1",
    "react-markdown": "^8.0.3",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^6.1.1",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "rss": "^1.2.2",
    "sass": "^1.51.0",
    "swr": "^1.3.0",
    "twemoji": "14.0.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/cz-commitlint": "^16.3.0",
    "@next/bundle-analyzer": "^12.1.6",
    "@notion-stuff/v4-types": "^6.0.0",
    "@types/cors": "^2.8.12",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-syntax-highlighter": "^15.5.1",
    "@types/rss": "^0.0.29",
    "@types/twemoji": "^13.1.2",
    "@types/umami": "^0.1.0",
    "commitizen": "^4.2.4",
    "commitlint": "^16.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-sznm": "^0.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "next-sitemap": "^2.5.20",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sozonome/sznm.dev"
  },
  "bugs": {
    "url": "https://github.com/sozonome/sznm.dev/issues"
  }
