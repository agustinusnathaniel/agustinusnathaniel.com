{
  "name": "sznm.dev",
  "private": true,
  "description": "Agustinus Nathaniel's dev site",
  "version": "1.27.0",
  "author": "sozonome",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "next build",
    "build:turbo": "npx turbo build",
    "analyze": "cross-env ANALYZE=true pnpm build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "type-check": "tsc --noEmit",
    "knip": "knip",
    "check:turbo": "npx turbo lint type-check",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "db:studio": "npx prisma studio",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@giscus/react": "^2.4.0",
    "@prisma/client": "^5.9.1",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@vercel/speed-insights": "^1.0.10",
    "cmdk": "^0.2.1",
    "framer-motion": "^11.0.5",
    "gray-matter": "^4.0.3",
    "lodash-es": "^4.17.21",
    "million": "3.0.3",
    "next": "14.1.0",
    "next-seo": "^6.5.0",
    "nextjs-toploader": "^1.6.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.0.1",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-wrap-balancer": "^1.1.0",
    "reading-time": "^1.5.0",
    "rehype-raw": "7.0.0",
    "remark-gfm": "^4.0.0",
    "rss": "^1.2.2",
    "sass": "^1.71.0",
    "swr": "^2.2.5",
    "ua-parser-js": "^1.0.37",
    "zod": "^3.22.4",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@commitlint/cz-commitlint": "18.6.1",
    "@next/bundle-analyzer": "^14.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/rss": "^0.0.32",
    "@types/ua-parser-js": "^0.7.39",
    "@vitest/coverage-v8": "^1.3.0",
    "@vitest/ui": "^1.3.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-sznm": "^2.0.2",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "husky": "^9.0.11",
    "knip": "^5.0.1",
    "lint-staged": "^15.2.2",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sozonome/sznm.dev"
  },
  "bugs": {
    "url": "https://github.com/sozonome/sznm.dev/issues"
  },
  "engines": {
    "node": ">=18.x",
    "pnpm": ">=8"
  }
}
